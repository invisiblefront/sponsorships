<h3>Add media</h3>	

<%= form_for(@new_asset, url: organizers_project_asset_path) do |f| %>
    <%= f.hidden_field :project_id, :value => current_organizer.project.id %>
<hr>
	<%= f.label :media_type %><br>
    <%= f.text_field :media_type, :value => "image" %>
<hr>
	<%= f.label :media %><br>
    <%= f.file_field :media %>
    
 <%= f.submit %>
<% end %>