<!DOCTYPE html>
<html>
<head>
  <title>YellowingsMc</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
</head>
<body>
	<div class="container">

	    <% if admin_signed_in? %>
	    	<%= render partial:"partials/hq_nav" %>
	    <% end %>
	    

	   	<% if organizer_signed_in? %>
	    	<%= render partial:"partials/organizer_nav" %>
	    <% end %>
	    
	    <%= render partial:"partials/device_notifications" %>
	    
		<%= yield %>
	</div>
</body>
</html>
