<!DOCTYPE html>
<html>
<head>
  <title>YellowingsMc</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="yw-upper-nav-container">
	<%= render partial:"partials/upper_nav" %>
	</div>

	<div class="container">
	    <% if admin_signed_in? %>
	    	<br>
	    <% elsif organizer_signed_in? %>
	    	<br>
	    <% elsif sponsor_signed_in? %>
	    	<br>
	    <% else %>
	    	<%= render partial:"partials/lp_nav" %>
	    <% end %>
	    	<%= render partial:"partials/device_notifications" %>
			<%= yield %>
	</div>
</body>
</html>
